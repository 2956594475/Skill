<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.laoxing.skill.dao.GoodsDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.laoxing.skill.entity.Goods">
        <id column="goods_id" property="gid" />
        <result column="goods_name" property="gname" />
        <result column="goods_price" property="gprice" />
        <result column="goods_stock" property="gstock" />
        <result column="goods_img" property="gimg" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        goods_id, goods_name, goods_price, goods_stock, goods_img
    </sql>



</mapper>
